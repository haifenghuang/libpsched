#!/bin/sh

if [ `id -u` -ne 0 ]; then
	echo "Current user is not root"
	exit 1
fi

if [ -e ./uconf/params/default/undo ]; then
	while read cmd; do
		/bin/sh -c "${cmd}"

		if [ $? -ne 0 ]; then
			echo "Failed to execute: ${cmd}"
			exit 1
		fi
	done < ./uconf/params/default/undo
fi

if [ -e ./uconf/params/`cat .osname 2> /dev/null`/undo ]; then
	while read cmd; do
		/bin/sh -c "${cmd}"

		if [ $? -ne 0 ]; then
			echo "Failed to execute: ${cmd}"
			exit 1
		fi
	done < ./uconf/params/`cat .osname`/undo
fi

rm -rf build
rm -f .target
rm -f .osname
rm -f .extlib
rm -f .compiler
rm -f .olinker
rm -f .ecflags
rm -f .elflags
rm -f .archflags
rm -f .addons
rm -f .done
rm -f .l*

